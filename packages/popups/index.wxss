/* packages/popups/index.wxss */
.popups{position:fixed;top:0;left:0;right:0;bottom:0;background:rgba(0, 0, 0, 0.65);display:none;}
.content-view{position:fixed;background: #fff;display:none;}
.show{display: block;}
.content-center{
  top:50%;left:50%;
-webkit-transform:translate3d(-50%,-50%,0);transform:translate3d(-50%,-50%,0);
min-width:125px;min-height:90px;border-radius:6px;text-align:center;
}
.content-bottom{bottom:0;}